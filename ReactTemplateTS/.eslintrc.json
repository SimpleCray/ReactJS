{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "plugin:import/typescript", "plugin:prettier/recommended"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "indent": 0,
    "react/prop-types": 0,
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": 0,
    "linebreak-style": 0,
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 100
      }
    ]
  }
}
